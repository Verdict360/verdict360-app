# Verdict360 Legal Chatbot Environment Variables

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_API_VERSION=v1

# Keycloak Authentication  
VITE_KEYCLOAK_URL=http://localhost:8080
VITE_KEYCLOAK_REALM=Verdict360
VITE_KEYCLOAK_CLIENT_ID=legal-chatbot-web

# Legal Services
VITE_LEGAL_CHAT_ENABLED=true
VITE_CONSULTATION_BOOKING_ENABLED=true
VITE_VOICE_CALL_ENABLED=true

# Application Settings
VITE_APP_NAME="Verdict360"
VITE_APP_DESCRIPTION="AI Legal Chatbot Platform for South African Law Firms"
VITE_SUPPORT_EMAIL="support@verdict360.co.za"
VITE_SUPPORT_PHONE="+27 11 123 4567"

# Feature Flags
VITE_ANALYTICS_ENABLED=true
VITE_EXPORT_ENABLED=true
VITE_MULTI_LANGUAGE_ENABLED=false